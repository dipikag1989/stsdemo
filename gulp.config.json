  "vendorjs": [
    "./html/lib/ionic/release/js/ionic.bundle.js",
    
    "./html/lib/ngCordova/dist/ng-cordova.js"
  ],
  "vendorcss": [
    "./html/lib/ionicons/css/ionicons.css"
  ],
  "css": [
